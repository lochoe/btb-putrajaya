{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Index.html"
    },
    {
      "src": "/jersey",
      "dest": "/Index.html"
    },
    {
      "src": "/players",
      "dest": "/Index.html"
    }
  ]
}
